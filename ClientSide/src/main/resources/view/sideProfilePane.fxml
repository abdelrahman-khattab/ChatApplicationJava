<ScrollPane fitToHeight="true" fitToWidth="true" minWidth="220.0" prefWidth="301.0" style="-fx-background-color: #303841;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iti.project.presentation.controllers.ProfileController">
