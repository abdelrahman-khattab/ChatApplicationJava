.sidebar-background {
  -fx-background-color: #36404a;
  -fx-background-radius: 15px 0px 0px 15px;
}
.list-chat-background {
  -fx-background-color: #303841;
  -fx-border-width: 0 0.2px 0 00.2px;
  -fx-border-color: #bec2c6;
}
.top-chat
{
-fx-background-color:#2E353E;}
.button-icon:hover {
  -fx-background-color: #6874cc;
}
.icons {
  -fx-background-color: transparent;
}
.smooth-border {
  -fx-background-radius: 15px 15px 15px 15px;
}
.textname {
  -fx-font-family: Public Sans, sans-serif;
  -fx-font-weight: bold;
  -fx-font-size: 18px;
  -fx-fill: #e1e9f1;
}
.pane-line {
  -fx-background-color: #e1e9f1;
}
.text-field {
  -fx-background-color: #36404a;
}
.main-chat-background {
  -fx-background-color: #262e35;
}
.menu-border {
  -fx-background-radius: 10px;
}
.button-icon-radius {
  -fx-background-color: transparent;
  -fx-background-radius: 20 0 20 20;
}
.button-pressing:pressed {
  -fx-background-color: #CBCF92;
  -fx-background-radius: 20;
  -fx-border-color: transparent;
}
.send-btn-background {
  -fx-background-color: #fdfeff;
}
.send-btn-background-pressing:pressed {
  -fx-background-color: #6874cc;
}
.custom-text-field:focused {
  -fx-border-width: 2;
  -fx-border-color: #ccd0d4;
   -fx-fill:white;
}
.side-bar-buttons:hover {
  -fx-border-radius: 20;
  -fx-border-color: #CBCF92;
}
.mouse-hand {
  -fx-cursor: hand;
}
.text-formates-background {
  -fx-background-color: transparent;
}
.text-formates-button:hover {
  -fx-background-color:#CBCF92;
}
.input-text
{
-fx-text-fill:white;
}


.scroll-bar:vertical{
    -fx-background-color:transparent;
}

.increment-button, .decrement-button {
    -fx-background-color: transparent;
    -fx-border-color: transparent;
}

.scroll-bar:vertical .track,
.scroll-bar:vertical .track{
    -fx-background-color: transparent;
    -fx-border-color: transparent;
    -fx-background-radius: 0em;
}

.scroll-bar:vertical .thumb {
    -fx-background-color: white;
    -fx-background-radius: 5em;
}



.choice-box > .label { -fx-text-fill: white; }


.choice-box{
    -fx-background-color: transparent;
	  -fx-mark-color: #CBCF92;
	  -fx-border-width:0.5;
	  -fx-border-color:#CBCF92
}
.color-picker {
    -fx-background-color: transparent;
		  -fx-mark-color: #CBCF92;
	  -fx-border-width:0.5;
	  -fx-border-color:#CBCF92
}